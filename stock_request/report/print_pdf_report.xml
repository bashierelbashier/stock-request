<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="print_pdf_report" model="ir.actions.report">
        <field name="name">Print Pdf Report</field>
        <field name="model">stock.request</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">stock_request.report_stock_request_pdf_report</field>
        <field name="report_file">stock_request.report_stock_request_pdf_report</field>
        <field name="binding_model_id" ref="model_stock_request"/>
