    <record id="print_excel_report" model="ir.actions.report">
        <field name="name">Print Excel Report</field>
        <field name="model">stock.request</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">stock_request.report_stock_request_excel_report_xls</field>
        <field name="report_file">stock_request.report_stock_request_excel_report_xls</field>
    </record>
